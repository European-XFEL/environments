#%Module 1.0
proc ModulesHelp {} {
    puts stdout    "Mamba environment for cycle 202302"
}

module-whatis  "Module loads the mamba environment for cycle 202302"

if { [ module-info mode load ] } {
    system    "/gpfs/exfel/sw/software/local/etc/metrics.sh 'mamba/202302'"
}

module load mambaforge

prepend-path    PATH /gpfs/exfel/sw/software/mambaforge/22.11/envs/202302/bin
prepend-path    XML_CATALOG_FILES file {///gpfs/exfel/sw/software/mambaforge/22.11/envs/202302/etc/xml/catalog file} ///etc/xml/catalog
setenv          CONDA_DEFAULT_ENV 202302
setenv          CONDA_PREFIX /gpfs/exfel/sw/software/mambaforge/22.11/envs/202302
setenv          CONDA_PROMPT_MODIFIER {(202302) }
setenv          CONDA_SHLVL 1
setenv          GSETTINGS_SCHEMA_DIR /gpfs/exfel/sw/software/mambaforge/22.11/envs/202302/share/glib-2.0/schemas
setenv          GSETTINGS_SCHEMA_DIR_CONDA_BACKUP {}

